{
    "name": "honwei189/html",
    "description": "HTML collective for PHP",
    "authors": [
        {
            "name": "Gordon Lim",
            "email": "honwei189@gmail.com"
        }
    ],
    "require": {
        "honwei189/flayer": "^1.0",
        "honwei189/fdo": "^1.0",
        "php": ">=7.3.0"
    },
    "autoload": {
        "psr-4": {
            "honwei189\\html\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "honwei189\\html\\htmlServiceProvider"
            ]
        },
        "installer-paths": {
            "./vendor/{$vendor}/honwei189/libs/{$name}": [
                "honwei189/html"
            ]
        }
    }
}

